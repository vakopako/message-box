{
  "collectCoverageFrom": [
    "src/**/*.{js,jsx,ts,tsx}",
    "!src/**/*.d.ts"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/e2e/"
  ],
  "transform": {
    "^.+\\.(t|j)sx?$": "@swc/jest"
  },
  "setupFilesAfterEnv": [
    "./jestSetup.ts"
  ],
  "testEnvironment": "./jestTestEnvironment.ts"
}